# ==============================================
# Wildlife Valparai Environment Variables
# ==============================================
# Copy this file to .env and fill in your values
# NEVER commit .env to git!
# ==============================================

# --- Cloudflare R2 Storage ---
VITE_R2_BUCKET_NAME="your-bucket-name"
VITE_R2_ACCESS_KEY_ID="your-r2-access-key-id"
VITE_R2_SECRET_ACCESS_KEY="your-r2-secret-access-key"
VITE_CLOUDFLARE_ACCOUNT_ID="your-cloudflare-account-id"
VITE_R2_PUBLIC_URL="https://your-r2-public-url.r2.dev"

# --- Cloudflare Worker ---
VITE_WORKER_URL="https://your-worker.workers.dev"

# --- Firebase Configuration ---
VITE_FIREBASE_API_KEY="your-firebase-api-key"
VITE_FIREBASE_AUTH_DOMAIN="your-project.firebaseapp.com"
VITE_FIREBASE_PROJECT_ID="your-project-id"
VITE_FIREBASE_STORAGE_BUCKET="your-project.firebasestorage.app"
VITE_FIREBASE_MESSAGING_SENDER_ID="your-messaging-sender-id"
VITE_FIREBASE_APP_ID="your-firebase-app-id"
VITE_FIREBASE_MEASUREMENT_ID="G-XXXXXXXXXX"

# --- Google Ads ---
VITE_GOOGLE_ADS_CLIENT_ID="your-adsense-client-id"

# --- EmailJS (Sponsor Form) ---
VITE_SPONSOR_SERVICE_ID="your-emailjs-service-id"
VITE_SPONSOR_TEMPLATE_ID="your-emailjs-template-id"
VITE_SPONSOR_PUBLIC_KEY="your-emailjs-public-key"

# --- EmailJS (Contact Form) ---
VITE_CONTACT_SERVICE_ID="your-emailjs-service-id"
VITE_CONTACT_TEMPLATE_ID="your-emailjs-template-id"
VITE_CONTACT_PUBLIC_KEY="your-emailjs-public-key"
